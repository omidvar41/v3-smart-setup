<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#f5f4c2" />

            <corners android:radius="0dip" />
        </shape></item>
    <item android:bottom="5dip"><shape>
            <solid android:color="#0050bc" />

            <stroke android:width="1dip" android:color="#0050bc" />

            <corners android:bottomLeftRadius="0dip" android:bottomRightRadius="0dip" android:topLeftRadius="0dip" android:topRightRadius="0dip" />
        </shape></item>

</selector>