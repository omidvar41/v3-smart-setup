<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#267200" />

            <corners android:radius="10dip" />
        </shape></item>
    <item android:bottom="5dip"><shape>
            <solid android:color="#349f00" />

            <stroke android:width="1dip" android:color="#1b5300" />

            <corners android:bottomLeftRadius="0dip" android:bottomRightRadius="10dip" android:topLeftRadius="10dip" android:topRightRadius="0dip" />
        </shape></item>

</selector>